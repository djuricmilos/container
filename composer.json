{
    "name": "djuricmilos/container",
    "description": "Dependency injection container",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "container",
        "dependency",
        "injection",
        "di"
    ],
    "authors": [
        {
            "name": "Miloš Đurić",
            "email": "djuric.milos@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laganica\\Di\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Laganica\\Di\\Test\\": "tests/"
        },
        "files": ["src/functions.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
