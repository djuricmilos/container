{
    "name": "djuricmilos/container",
    "description": "Dependency injection container",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "container",
        "container-interop",
        "dependency",
        "injection",
        "dependency injection",
        "di",
        "ioc",
        "psr-11",
        "psr11"
    ],
    "authors": [
        {
            "name": "Miloš Đurić",
            "email": "djuric.milos@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "psr/container": "^1.0",
        "php-di/phpdoc-reader": "^2.1",
        "phpdocumentor/reflection-docblock": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Laganica\\Di\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Laganica\\Di\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
